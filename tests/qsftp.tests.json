{
    "qsftp": {
        "routes": {
            "book_loan_v1": {
                "sequence": ["messageToTemplateVars",  "messageToPath", "uploadToSftpServer"],
                "warez": {
                    "messageToPath": {
                        "options": {
                            "template": "tests/{{message.properties.messageId}}.txt"
                        }
                    },
                    "uploadToSftpServer": {
                        "options": {
                            "hostname": "localhost",
                            "port": 10022,
                            "username": "fred",
                            "password": "password"
                        }
                    }
                }
            },
            "book_loan_v2": {
                "sequence": ["messageToTemplateVars", "messageTimestampToTemplateVar",  "messageToPath", "uploadToSftpServer"],
                "warez": {
                    "messageTimestampToTemplateVar": {
                        "options": {
                            "source": "/properties/headers/timestamp",
                            "destination": "timestamp",
                            "template": "YYYY-MM-DD"
                        }
                    },
                    "messageToPath": {
                        "options": {
                            "template": "tests/{{timestamp}}.txt"
                        }
                    },
                    "uploadToSftpServer": {
                        "options": {
                            "hostname": "localhost",
                            "port": 10022,
                            "username": "fred",
                            "password": "password"
                        }
                    }
                }
            }
        }
    },
    "rascal": {
        "vhosts": {
            "/": {
                "namespace": true,
                "exchanges": {
                    "e1": {}
                },
                "queues": {
                    "q1": {},
                    "q2": {}
                },
                "bindings": {
                    "b1": {
                        "source": "e1",
                        "destination": "q1",
                        "bindingKey": "library.v1.#"
                    },
                    "b2": {
                        "source": "e1",
                        "destination": "q2",
                        "bindingKey": "library.v2.#"
                    }
                }
            }
        },
        "publications": {
            "p1": {
                "exchange": "e1"
            }
        },
        "subscriptions": {
            "s1": {
                "queue": "q1"
            }
        },
        "subscriptions": {
            "s2": {
                "queue": "q2"
            }
        }
    }
}